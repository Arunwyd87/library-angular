{
  "name": "ui-sdk",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "run-p lib:watch showcase:start-waiton",
    "build": "run-s lib:build showcase:build",
    "test": "run-p lib:test showcase:test",
    "lib:build": "ng build",
    "lib:watch": "ng build  --watch --configuration development ",
    "lib:test": "ng test",
    "showcase:start-waiton": "wait-on dist/mycomp/ui-sdk/package.json && npm run showcase:start",
    "showcase:start": "ng serve --project showcase",
    "showcase:build": "ng build --project showcase",
    "showcase:watch": "ng build --project showcase --watch --configuration development",
    "showcase:test": "ng test --project showcase"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.5",
    "@angular/cli": "~13.2.5",
    "@angular/compiler-cli": "~13.2.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.5.2",
    "wait-on": "^6.0.1"
  }
}
